#include <stdio.h>
int main() 
{
    int a[] = {1, 2, 4, 5, 7};
    int b[] = {5, 6, 3, 4, 8};
	int n=sizeof(a)/sizeof(a[0]);
	int k=9;
	for(int i=0;i<n;i++)
	{
	    for(int j=0;j<n;j++)
	    {
	        if((a[i]+a[j])==8)
	        {
	            printf("%d,%d",a[i],b[j]);
	            printf("\n");
	        }
	    }
	}
    

    return 0;
}
