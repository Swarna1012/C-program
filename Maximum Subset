#include<stdio.h>
int main()
{
    int arr[]={5,8,-1,4};
    //int arr[]={4,2,-3,3,-2,-2,8};
    //int arr[]={10,20,30,40,50,60};
    //int arr[]={-1,-2,-3,-4,-5};
    //int arr[]={2,4,6,8,10};
    //int arr[]={5, 8, 5, 4};
    //int arr[]={4, 2, -3, 3, -2, -2, 8};
    int n=sizeof(arr)/sizeof(arr[0]);
    int set1=0,set2=0;

    for(int i=0;i<n;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(arr[i]==arr[j])
            {
                arr[i]=arr[j]=0;
            }
        }
        if(arr[i]>0)
        {
            set1=set1+arr[i];
        }
        else
            set2=set2+arr[i];
    }

    for(int i=0;i<n;i++)
    {
        printf("%d ",arr[i]);
    }

    printf("\n%d ",set1);
    printf("\n%d ",set2);

    printf("\nMaximum is %d ",set1-set2);
}
